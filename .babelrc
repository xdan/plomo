{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "last 2 versions",
            "ie 10"
          ]
        },
        "debug": false
      }
    ],
    "react",
    "es2015-ie"
  ],
  "plugins": [
    "transform-object-assign",
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}